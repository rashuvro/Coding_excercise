class Solution:
    def diStringMatch(self, S):
        c=list(S)
        d=len(c)
        e = []
        f= []
        g=0
        for i in range (0,d):
            if c[i]=='I':
                f.append(g)
                g=g+1
            elif c[i]=='D':
                f.append(d)
                d=d-1
        f.append(g)
        return f
        
